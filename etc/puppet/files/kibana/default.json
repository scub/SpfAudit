        "0": {
          "query": "type:Node && (( mx_hosted:Google && spf_method:None )|( mx_hosted:External && spf_method:Pass ))",
          "alias": "Vulnerable Hosts",
          "color": "#890F02",
          "id": 0,
          "pin": true,
          "type": "lucene",
          "enable": true
        },
        "1": {
          "id": 1,
          "color": "#E5AC0E",
          "alias": "Suspect Hosts",
          "pin": true,
          "type": "lucene",
          "enable": true,
          "query": "type:Node && ((mx_hosted:External && spf_method:None )|(( mx_hosted:External | mx_hosted:Google ) && ( spf_method:Neutral | spf_method:SoftFail )))"
        },
        "2": {
          "id": 2,
          "color": "#629E51",
          "alias": "Secure hosts",
          "pin": true,
          "type": "lucene",
          "enable": true,
          "query": "type:Node && ( mx_hosted:Internal |(( mx_hosted:External | mx_hosted:Google ) && ( spf_method:HardFail | spf_method:Fail )))"
        },
        "3": {
          "id": 3,
          "color": "#508642",
          "alias": "Internal Secure",
          "pin": true,
          "type": "lucene",
          "enable": true,
          "query": "type:Node && mx_hosted:Internal"
        },
        "4": {
          "id": 4,
          "color": "#DEDAF7",
          "alias": "All Hosts",
          "pin": true,
          "type": "lucene",
          "enable": true,
          "query": "type:Node"
        }
      },
      "ids": [
        0,
        1,
        2,
        3,
        4
      ]
    },
